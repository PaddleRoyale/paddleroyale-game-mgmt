package  paddleRoyale{		import flash.display.MovieClip;	import flash.geom.Point;	import paddleRoyale.PaddleRoyaleModel;	import com.greensock.TweenMax;	import com.greensock.easing.*;		public class PlayerCueClass extends MovieClip{		public var point:Point;		private var _model:PaddleRoyaleModel;		public function PlayerCueClass() {			point = new Point(this.x, this.y);			_model = PaddleRoyaleModel.getInstance();			this.y = this.stage.stageHeight + this.height;		}				public function showNextUp(){			TweenMax.to(this, 1, {y:point.y});		}				public function hideNextUp(){			TweenMax.to(this, 1, {y:this.stage.stageHeight + this.height, onComplete:clearText});		}				public function clearText():void{			this.playerCue.text = "";		}						public function transitionNextUp():void{			TweenMax.to(this, .25, {y:this.stage.stageHeight + this.height, onComplete:clearTextGoBackUp});		}				public function clearTextGoBackUp():void{			TweenMax.delayedCall(.5,showNextUp);		}	}	}