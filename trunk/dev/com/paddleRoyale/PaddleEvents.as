package  paddleRoyale{		import flash.events.Event;		public class PaddleEvents extends Event{		public static const XML_LOADED:String = "XML Loaded";		public static const XML_UPDATED:String = "XML Updated";		private var _name:*;		private var _objName:String;				public function PaddleEvents(type:String, __name:*=null, __objName:String=null) {			super(type);			this._name = __name;			this._objName = __objName;		}		public function get objectName():String		{			return _objName;		}				public function get name():*		{			return _name;		}	}	}