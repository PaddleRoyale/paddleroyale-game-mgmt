package  paddleRoyale{		import paddleRoyale.PaddleRoyaleModel;		public class PaddleUtilClass {		private var _model:PaddleRoyaleModel;			public function PaddleUtilClass() {			_model = PaddleRoyaleModel.getInstance();		}		public function sanitizeString(word:String):String{			var tempHolder:Array=word.split("");			var sanitizedWord:String="";			for(var i=0; i<tempHolder.length; i++){				if(tempHolder[i] == " " || tempHolder[i] == ""){					tempHolder.splice(i,1);				}else if(tempHolder[i] == '"'){					tempHolder.splice(i,0);				}else{					sanitizedWord+=tempHolder[i];				}			}			return sanitizedWord;		}				public function sanitizeArray(dataset:Array):Array{			for(var i=0; i<dataset.length; i++){				if(dataset[i] == "" || dataset[i] == " " ){					dataset.splice(i,1);				}			}			return dataset;		}	}	}